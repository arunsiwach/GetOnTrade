                        function GetSortOrder(prop) {
                            return function (a, b) {
                                if (a[prop] > b[prop]) {
                                    return 1;
                                } else if (a[prop] < b[prop]) {
                                    return -1;
                                }
                                return 0;
                            }
                        }

                        function GetReverseSortOrder(prop) {
                            return function (a, b) {
                                if (a[prop] < b[prop]) {
                                    return 1;
                                } else if (a[prop] > b[prop]) {
                                    return -1;
                                }
                                return 0;
                            }
                        }


                        function GetSortOrderDate(prop) {
                            return function (a, b) {
                                if (Date.parse(a[prop]) > Date.parse(b[prop])) {
                                    return 1;
                                } else if (Date.parse(a[prop]) < Date.parse(b[prop])) {
